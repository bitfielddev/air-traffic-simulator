{
  "name": "air-traffic-simulator-client",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev-server": "run-p dev server",
    "dev": "vite",
    "server": "cd .. && cargo run --package air-traffic-simulator-example --no-default-features",
    "build": "run-p type-check \"build-only {@}\" --",
    "preview": "vite preview",
    "build-only": "vite build",
    "type-check": "vue-tsc --build --force",
    "lint": "eslint --fix src/",
    "format": "prettier --write .",
    "gen-types": "cd ../engine && cargo test && $(mv bindings ../client/src || true) && cd ../client && eslint --fix src/bindings"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "^6.7.2",
    "@fortawesome/fontawesome-svg-core": "^6.7.2",
    "@fortawesome/free-solid-svg-icons": "^6.7.2",
    "@fortawesome/vue-fontawesome": "^3.0.8",
    "Leaflet.MultiOptionsPolyline": "^1.0.0",
    "leaflet": "^1.9.4",
    "leaflet-easybutton": "^2.4.0",
    "socket.io-client": "^4.8.1",
    "uuid": "^11.1.0",
    "vue": "^3.5.17"
  },
  "devDependencies": {
    "@eslint/js": "^9.29.0",
    "@tsconfig/node22": "^22.0.2",
    "@types/leaflet": "^1.9.18",
    "@types/node": "^24.0.3",
    "@types/uuid": "^10.0.0",
    "@vitejs/plugin-vue": "^5.2.4",
    "@vue/tsconfig": "^0.7.0",
    "eslint": "^9.29.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-vue": "^10.2.0",
    "globals": "^16.2.0",
    "npm-run-all2": "^8.0.4",
    "prettier": "^3.5.3",
    "typescript": "^5.8.3",
    "typescript-eslint": "8.34.1",
    "vite": "^6.3.5",
    "vite-plugin-vue-devtools": "^7.7.7",
    "vue-eslint-parser": "^10.1.3",
    "vue-tsc": "^2.2.10"
  }
}
